@using Microsoft.AspNetCore.Components;
@using Syncfusion.Blazor.Calendars.Internal
@typeparam TValue
@inherits CalendarBase<TValue>
@namespace Syncfusion.Blazor.Calendars
@using Syncfusion.Blazor.Internal;
<SyncfusionLicense></SyncfusionLicense>
<ResourceManager></ResourceManager>
<div id="@ID" @ref="Element" class="@RootClass" @attributes="containerAttr" tabindex="@this.TabIndex">
    <CascadingValue Value="@this">
            <CalendarBaseRender @ref="@CalendarBase" TValue="TValue" CssClass="@this.CssClass" Enabled="@this.Enabled" MultiSelection="@this.IsMultiSelection" @bind-MultiValues="@this.Values"></CalendarBaseRender>
        @ChildContent
    </CascadingValue>
</div>